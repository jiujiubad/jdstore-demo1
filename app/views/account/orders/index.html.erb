<h2>我的订单</h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>订单编号</th>
      <th>创建时间</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= link_to(order.id, order_path(order.token)) %></td>
        <td><%= order.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
