<h1>新增商品</h1>

<div class="col-md-6 col-center">
  <%= simple_form_for [:admin, @product] do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :quantity %>
    <%= f.input :image %>
    <%= f.submit "保存", data:{disable_with:"正在保存.."} %>
  <% end %>
</div>
