<%= f.input :title %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :quantity %>
<%= f.label :"多图上传" %>
<%= f.file_field :avatar, :multiple => true, name: "photos[avatar][]" %>
<br/>
<%= f.label :"选择分类" %>
<%= f.select :category_ids, Category.all.map{ |c| [c.name, c.id] }, {}, :multiple => true, :class => "form-control" %>
<%= f.input :detail, as: :ckeditor, language:'zh-CN', label:'图文详情' %>
